ENTRY(_start);

SECTIONS {
    {{#with stage3}}
    .body {{body}} : {  
        KEEP(*(.startup)) 
        KEEP(*(.real))
        KEEP(*(.stage_3))
        *(.text*)
    }

    .ldt {{ldt}} : {
        KEEP(*(.ldt))
    }
    .tss {{tss}} : {
        KEEP(*(.tss))
    }
    
    .data {{data}} : {
        *(.rodata*)
        *(.data*)
        *(.bss*)
    }
    {{/with}}

    /DISCARD/ : { *(.eh_frame*) *(.discard)}
}